          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  r.origin,\n  r.destination,\n  r.origin_lat,\n  r.origin_lon,\n  r.destination_lat,\n  r.destination_lon,\n  COUNT(*) as route_count\nFROM route_at_synced r\nWHERE r.origin_lat IS NOT NULL \n  AND r.origin_lon IS NOT NULL\n  AND r.destination_lat IS NOT NULL \n  AND r.destination_lon IS NOT NULL\nGROUP BY r.origin, r.destination, r.origin_lat, r.origin_lon, r.destination_lat, r.destination_lon\nORDER BY route_count DESC",
